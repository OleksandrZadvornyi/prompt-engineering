# LangSmith settings (optional)
LANGSMITH_TRACING=true  # Enable tracing for debugging in LangSmith
LANGSMITH_ENDPOINT=https://api.smith.langchain.com  # LangSmith API endpoint
LANGSMITH_API_KEY=<your_langsmith_api_key>  # LangSmith API key
LANGSMITH_PROJECT=default  # Project name in LangSmith

# OpenRouter settings (required)
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1  # OpenRouter API base URL
OPENROUTER_API_KEY=<your_openrouter_api_key>  # OpenRouter API key

